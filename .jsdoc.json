{
  "plugins": [],
  "recurseDepth": 10,
  "opts": {
    "recurse": true,
    "destination": "./jsdoc/"
  },
  "source": {
    "include": [
      "."
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "e2e|node_modules"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
